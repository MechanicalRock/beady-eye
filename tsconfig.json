{
 "compilerOptions": {
   "preserveConstEnums": true,
   "strictNullChecks": true,
   "sourceMap": true,
   "target": "es5",
   "outDir": ".build",
   "moduleResolution": "node",
   "lib": ["es2015", "es6", "esnext"],
   "allowJs": true,    
 },
 "rootDirs": [
    "./"
  ],
 "include": [
   "src",    
   "spec",
   "lambda",
   "src/*.js",
   "lambda/*.js"
 ],
 "exclude": [
   "node_modules",
   ".build"
 ]
}