{
  "name": "@mechanicalrock/beady-eye",
  "version": "0.17.6",
  "description": "",
  "main": ".build/src/index.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "build:watch": "nodemon --watch src --legacy-watch --ext ts --exec npm run build --",
    "clean": "rm -rf .build",
    "ci:upgrade-dependencies": "./upgrade-dependencies.sh",
    "dependency:upgrade": "npx npm-upgrade",
    "deploy:dev": "serverless deploy --stage dev",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "package": "serverless package",
    "preversion": "npm run build",
    "prepublish": "npm run build",
    "release:major": "npm run clean && npm run build && npm version major && npm run test && npm login && npm publish --access public && npm run test:security-monitor && git push && git push --tags",
    "release:minor": "npm run clean && npm run build && npm version minor && npm run test && npm login && npm publish --access public && npm run test:security-monitor && git push && git push --tags",
    "release:patch": "npm run clean && npm run build && npm version patch && npm run test && npm login && npm publish --access public && npm run test:security-monitor && git push && git push --tags",
    "security:fix": "snyk wizard",
    "test": "npm run build && jest",
    "test:approval": "jest --config test-data/jest.approval.json",
    "test:approval-old": "npm build && node .build/test-data/approval-test-generator.js",
    "test:watch": "jest --watch",
    "test:accept": "TMPDIR=/tmp cucumber-js spec",
    "test:security": "snyk test",
    "test:security-monitor": "snyk monitor",
    "test:cover": "jasmine --coverage",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MechanicalRock/beady-eye"
  },
  "author": "Tim Myerscough <tim.myerscough@mechanicalrock.io>",
  "contributors": [],
  "license": "Apache-2.0",
  "homepage": "https://github.com/MechanicalRock/beady-eye",
  "dependencies": {
    "chai": "^4.2.0",
    "date-fns": "^1.30.1",
    "jasmine": "^3.5.0",
    "jasmine-promise": "^0.0.1",
    "jasmine-reporters": "^2.3.2",
    "jasmine-ts-console-reporter": "^3.1.1",
    "lodash.ismatch": "^4.4.0",
    "proxy-agent": "^3.1.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.27",
    "@types/chai": "^4.1.7",
    "@types/jest": "^24.0.15",
    "@types/lodash.ismatch": "^4.4.6",
    "@types/node": "^11.13.7",
    "approvals": "^3.0.5",
    "aws-sdk-mock": "^4.5.0",
    "cloudformation-declarations": "^0.2.0",
    "jasmine-core": "^3.4.0",
    "jasmine-ts": "^0.3.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "snyk": "^1.316.1",
    "ts-jest": "^24.0.2",
    "typescript": "3.5.2"
  },
  "peerDependencies": {
    "aws-sdk": "^2.337.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/spec/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "testMatch": [
      "<rootDir>/test/**/?(*.)(spec|test).ts?x)",
      "<rootDir>/spec/**/?(*.)(spec|test).ts?(x)",
      "**/*.steps.ts?(x)"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "snyk": true
}
